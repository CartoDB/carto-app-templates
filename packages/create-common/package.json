{
  "name": "@carto/create-common",
  "packageManager": "yarn@4.2.2",
  "author": "Don McCurdy <donmccurdy@carto.com>",
  "version": "0.0.0",
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "tag": "alpha"
  },
  "type": "module",
  "sideEffects": false,
  "source": "src/create.ts",
  "types": "./build/create.d.ts",
  "main": "./build/create.cjs",
  "module": "./build/create.modern.js",
  "exports": {
    ".": {
      "types": "./build/create.d.ts",
      "require": "./build/create.cjs",
      "default": "./build/create.modern.js"
    },
    "./style.css": "./style.css"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "node >= 18"
  ],
  "scripts": {
    "build": "microbundle --format cjs,modern --no-compress --target node --define VERSION=$npm_package_version",
    "dev": "microbundle watch --format cjs,modern --no-compress --target node --define VERSION=$npm_package_version",
    "lint": "prettier \"src/**/*\" --check",
    "prepack": "yarn build",
    "prepublish": "yarn lint"
  },
  "dependencies": {
    "glob": "^11.0.0",
    "kolorist": "^1.8.0",
    "meow": "^13.2.0",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@types/meow": "^6.0.0",
    "@types/prompts": "^2.4.9",
    "microbundle": "^0.15.1"
  }
}
